<h2>Sign Up</h2>
<!-- my form -->

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

<div><%= f.label :Name %><br />
  <%= f.text_field :name %></div>

  <div><%= f.label :Mentor %><br />
  <%= f.text_field :mentor %></div>

  <div><%= f.label :Occupation %><br />
  <%= f.text_field :occupation %></div>

  <div><%= f.label :City %><br />
  <%= f.text_field :city %></div>

  <div><%= f.label :Country %><br />
  <%= f.text_field :country %></div>

  <div><%= f.label :Url %><br />
  <%= f.text_field :url %></div>

  <div><%= f.label :Phone %><br />
 <%= f.text_field :url %></div>

  <div><%= f.label :Skills %><br />
  <%= f.text_field :skills %></div>

  <div><%= f.label :Open_Position %><br />
  <%= f.text_field :position_hiring %></div>

    <div><%= f.label :Position_Description %><br />
  <%= f.text_field :position_description %></div>

  <div><%= f.label :Email %><br />
<%= f.email_field :email, :autofocus => true %></div>

  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.submit "Sign up" %></div>
<% end %>

<%= render "users/shared/links" %>
